{
  "name": "tfl",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run tsc",
    "start:android": "npm run build && concurrently -r 'react-native run-android' 'npm run ts'",
    "start:ios": "npm run build && concurrently -r 'react-native run-ios' 'npm run ts'",
    "start:native": "node node_modules/react-native/local-cli/cli.js start",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ts": "./node_modules/.bin/tsc --watch --pretty",
    "tsc": "node_modules/.bin/tsc",
    "tslint": "tslint --fix -e \"**/*.snap\"",
    "lint": "npm run tslint \"src/**/*.ts*\"",
    "test:unit": "jest --config=jest.config.json",
    "pretest:unit": "npm run lint && ./node_modules/.bin/tsc --noUnusedLocals --noUnusedParameters"
  },
  "dependencies": {
    "@types/underscore": "^1.8.8",
    "axios": "^0.18.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "react": "16.3.1",
    "react-native": "0.55.2",
    "react-navigation": "^1.5.11",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "remote-redux-devtools": "^0.5.12",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.107",
    "@types/react": "^16.3.10",
    "@types/react-dom": "^16.0.5",
    "@types/react-native": "^0.55.2",
    "@types/react-navigation": "^1.5.5",
    "@types/react-redux": "^5.0.16",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux": "^3.6.0",
    "@types/redux-immutable": "^3.0.38",
    "@types/remote-redux-devtools": "^0.5.2",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "22.4.3",
    "jest-preset-typescript-react-native": "^1.2.0",
    "jest-transform-stub": "^1.0.0",
    "react-dom": "^16.3.1",
    "react-test-renderer": "^16.3.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "typings": "^2.1.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
